@model List<DevTutorialWebApp.Models.Roadmap>
@{
    ViewData["Title"] = "Data Science & AI";
}

<div class="container mt-4">
    <div class="row mb-5">
        <div class="col-lg-8">
            <h1 class="display-4">Data Science & AI</h1>
            <p class="lead">Unlock insights from data and build intelligent systems using machine learning and artificial intelligence.</p>
        </div>
    </div>

    <div class="row">
        @foreach (var roadmap in Model)
        {
            <partial name="_RoadmapCard" model="roadmap" />
        }
    </div>
</div>